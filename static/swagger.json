        ],
        "parameters": [
          {
            "in": "formData",
            "type": "string",
            "name": "Select Table",
            "description": "Select table to insert data",
            "enum": ["departments", "hired_employees", "jobs"],
            "example": "modded",
            "required": true              
          },
          {
            "name": "Upload File",
            "in": "formData",
            "type": "file",
            "description": "Dataframe in csv Format for Inserting Value",
            "required": true
          },
          {
            "name": "Save File",
            "in": "formData",
            "type": "boolean",
            "description": "True or False to upload File to DataBase",
            "default": true,
            "required": true
          }
          
        ],
        "responses": {
          "200": {
            "description": "Successful response.",
            "schema": {
              "type": "object",
              "properties": {
                "columns": {
                  "type": "array",
                  "items":{
                    "type": "string"
                  }
                },
                "index": {
                  "type": "array",
